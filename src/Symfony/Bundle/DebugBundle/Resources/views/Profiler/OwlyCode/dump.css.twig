#sf-dump pre
{
    white-space: pre;
}

li.sf-dump
{
    list-style-type: disc;
    border: none;
}

.sf-dump>pre
{
    margin: 15px 0;
    padding-left: 11px;
    width: 100%;
    overflow: hidden;
}

.sf-dump ol>li
{
    padding: 0;
}

.sf-dump-event br /* Hide Chrome inserted BR */
{
    display: none;
}

.sf-dump-event
{
    color: gray;
    margin: 0;
    padding: 0;
    font-style: italic;
    font-family: inherit;
}

.sf-dump-compact
{
    white-space: nowrap; /* This rule makes IE slow as hell */
}

.sf-dump-arrow,
.sf-dump-const,
.sf-dump-class,
.sf-dump-alias,
.sf-dump-handle,
.sf-dump-string
{
    color: black;
    font-style: normal;
    font-family: sans-serif;
}

.sf-dump a
{
    display: inline-block;
    position: relative;
    right: 15px;
    width: 0;
    font-style: normal;
    font-family: sans-serif;
    color: lightblue;
    cursor: pointer;
    text-decoration: none;
}

.sf-dump>a
{
    right: 0;
}

.sf-dump-const,
.sf-dump a:hover
{
    color: blue;
    text-decoration: none;
}

.sf-dump-string
{
    color: purple;
}

.sf-dump-empty.sf-dump-string
{
    width: 3px;
    height: 1em;
    display: inline-block;
}

.sf-dump-key, .sf-dump-key *
{
    color: gray;
    font-style: italic;
}

.sf-dump-public
{
    color: green;
}

.sf-dump-protected
{
    color: orange;
}

.sf-dump-private
{
    color: red;
}

.sf-dump-arrow
{
    color: gray;
    background-color: white;
}

.sf-dump-event.sf-dump-silenced>.sf-dump-compact span
{
    color: silver;
    background-color: white;
}

.sf-dump-compact a,
.sf-dump-compact .sf-dump-key
{
    display: none;
}

.sf-dump-key.sf-dump-string
{
    display: inline;
}

.sf-dump-compact>ol
{
    display: none;
}

.sf-dump-undefined {
    color: gray;
    font-style: italic;
}

.sf-dump-blackbox {
    background-color: black;
    margin: 20px 0;
    color: white;
    padding: 10px;
    border-radius: 5px;
    word-wrap: break-word;
}

.sf-dump-blackbox * {
    font-family: Arial;
}

.sf-dump-blackbox li{
    padding-bottom: 0;
}

.sf-dump-colon {
    color: white;
}

.sf-dump-truncated {
    font-style: italic;
    color: #CCC;
}

.sf-dump-reference-marker {
    color: gray;
    padding: 0 2px;
    margin-right: 1px;
}

.sf-dump-hovered {
    cursor: help;
}

.sf-dump-hovered .sf-dump-reference-marker {
    color: yellow;
    background-color: #333;
    border-radius: 2px;
}

a[data-helper]:hover:after, span.sf-dump-string[data-helper]:hover:after {
  content: attr(data-helper);
  position: absolute;
  background-color: white;
  color: #333;
  padding: 5px;
  border-radius: 3px;
  margin-left: 10px;
}
