#sf-dump pre
{
    white-space: pre;
}

li.sf-dump
{
    list-style-type: disc;
    border: none;
}

.sf-dump>pre
{
    margin: 15px 0;
    padding-left: 11px;
    width: 100%;
    overflow: hidden;
}

.sf-dump ol>li
{
    padding: 0;
}

.sf-dump-event br /* Hide Chrome inserted BR */
{
    display: none;
}

.sf-dump-event
{
    color: gray;
    margin: 0;
    padding: 0;
    font-style: italic;
    font-family: inherit;
}

.sf-dump-compact
{
    white-space: nowrap; /* This rule makes IE slow as hell */
}

.sf-dump-arrow,
.sf-dump-const,
.sf-dump-class,
.sf-dump-alias,
.sf-dump-handle,
.sf-dump-string
{
    color: black;
    font-style: normal;
    font-family: sans-serif;
}

.sf-dump-class
{
    color: #D7AF00;
}

.sf-dump-const
{
    font-weight:bold;
    color:#0087FF;
}

.sf-dump-alias,
.sf-dump-handle
{
    color: #444444;
}

.sf-dump a
{
    display: inline-block;
    position: relative;
    right: 15px;
    width: 0;
    font-style: normal;
    font-family: sans-serif;
    color: lightblue;
    cursor: pointer;
    text-decoration: none;
}

.sf-dump>a
{
    right: 0;
}

.sf-dump-const,
.sf-dump a:hover
{
    color: blue;
    text-decoration: none;
}

.sf-dump-string
{
    font-weight:bold;
    color:#00D7FF;
    background-color: #F9F9F9;
}

.sf-dump-empty.sf-dump-string
{
    width: 3px;
    height: 1em;
    display: inline-block;
}

.sf-dump-key
{
    font-family: monospace;
    font-weight:normal;
    background-color: white;
    color: #005FFF;
}

.sf-dump-public
{
    color: #008700;
}

.sf-dump-protected
{
    color: #D75F00;
}

.sf-dump-private
{
    color: #D70000;
}

.sf-dump-arrow
{
    color: gray;
    background-color: white;
}

.sf-dump-event.sf-dump-silenced>.sf-dump-compact span
{
    color: silver;
    background-color: white;
}

.sf-dump-compact a,
.sf-dump-compact .sf-dump-key
{
    display: none;
}

.sf-dump-key.sf-dump-string
{
    display: inline;
}

.sf-dump-compact>ol
{
    display: none;
}
