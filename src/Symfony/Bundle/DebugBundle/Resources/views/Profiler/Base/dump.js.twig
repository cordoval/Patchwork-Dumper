(function (scope, document) {
    "use strict";

    var dump = scope.dump || {};
    scope.dump = dump;

    dump.toggle = function(a) {
        "use strict";

        var s = a.nextElementSibling.nextElementSibling;

        if ('sf-dump-compact' == s.className) {
            a.innerHTML = ' ⊟ ';
            s.className = 'sf-dump-expanded';
        } else {
            a.innerHTML = ' ⊞ ';
            s.className = 'sf-dump-compact';
        }
    }

})(Sfjs, document);
